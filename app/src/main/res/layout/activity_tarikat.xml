<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#23232A"
    tools:context=".TarikatActivity">


 <Button
     android:id="@+id/hometarikat"
     android:layout_width="130dp"
     android:layout_height="130dp"
     android:background="@drawable/circle_5"
     app:layout_constraintBottom_toBottomOf="parent"
     app:layout_constraintEnd_toEndOf="parent"
     app:layout_constraintHorizontal_bias="0.498"
     app:layout_constraintStart_toStartOf="parent"
     app:layout_constraintTop_toBottomOf="@+id/vird" />

 <Button
     android:id="@+id/vird"
     android:layout_width="180dp"
     android:layout_height="180dp"
     android:layout_marginStart="20dp"
     android:layout_marginTop="4dp"
     android:background="@drawable/rectangle_3"
     app:layout_constraintEnd_toStartOf="@+id/silsila"
     app:layout_constraintStart_toStartOf="parent"
     app:layout_constraintTop_toBottomOf="@+id/rabita" />

 <Button
     android:id="@+id/count"
     android:layout_width="180dp"
     android:layout_height="180dp"
     android:layout_marginTop="68dp"
     android:layout_marginEnd="20dp"
     android:layout_marginBottom="68dp"
     android:background="@drawable/rectangle_2"
     app:layout_constraintBottom_toTopOf="@+id/silsila"
     app:layout_constraintEnd_toEndOf="parent"
     app:layout_constraintStart_toEndOf="@+id/rabita"
     app:layout_constraintTop_toTopOf="parent" />

 <Button
     android:id="@+id/rabita"
     android:layout_width="180dp"
     android:layout_height="180dp"
     android:layout_marginStart="20dp"
     android:layout_marginTop="68dp"
     android:layout_marginBottom="68dp"
     android:background="@drawable/rectangle_1"
     app:layout_constraintBottom_toTopOf="@+id/vird"
     app:layout_constraintEnd_toStartOf="@+id/count"
     app:layout_constraintStart_toStartOf="parent"
     app:layout_constraintTop_toTopOf="parent" />

 <Button
     android:id="@+id/silsila"
     android:layout_width="180dp"
     android:layout_height="180dp"
     android:layout_marginTop="4dp"
     android:layout_marginEnd="20dp"
     android:background="@drawable/rectangle_4"
     app:layout_constraintEnd_toEndOf="parent"
     app:layout_constraintStart_toEndOf="@+id/vird"
     app:layout_constraintTop_toBottomOf="@+id/count" />


</androidx.constraintlayout.widget.ConstraintLayout>